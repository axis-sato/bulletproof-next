{
  "name": "bulletproof-next",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3001",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier -c \"**/*.+(js|json|ts|tsx)\"",
    "format:fix": "prettier -c --write \"**/*.+(js|json|ts|tsx)\"",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "validate": "run-p check-types && lint-staged",
    "prepare": "husky install",
    "aspida:rm": "rm -rf ./src/lib/api/aspida",
    "aspida:build": "openapi2aspida",
    "api:build": "run-s aspida:rm aspida:build",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@next/bundle-analyzer": "^12.0.7",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "framer-motion": "^4",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-table": "^7.7.0",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-table": "^7.7.9",
    "eslint": "8.4.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "msw": "^0.36.3",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.16.5",
    "pathpida": "^0.17.0",
    "prettier": "^2.5.1",
    "typescript": "4.5.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
